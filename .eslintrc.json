{
    "env": {
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "App": "true",
        "getApp": "true",
        "wx": "true",
        "Page": "true",
        "Component": "true"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": "off"
    },
    "settings": {
        "import/ignore": {
            "node_modules": true
        }
    }
}